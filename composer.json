{
    "name": "callcocam/tenant",
    "description": "This is my package tenant filament v3 for laravel",
    "keywords": [
        "callcocam",
        "laravel",
        "tenant",
        "filament",
        "filament-admin",
        "filament-admin-panel",
        "filament-admin-dashboard", 
        "filament-v3"
    ],
    "homepage": "https://github.com/callcocam/tenant",
    "support": {
        "issues": "https://github.com/callcocam/tenant/issues",
        "source": "https://github.com/callcocam/tenant"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Claudio Campos",
            "email": "callcocam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0",
        "illuminate/contracts": "^10.0",
        "leandrocfe/filament-ptbr-form-fields": "^3.0",
        "doctrine/dbal": "^3.7",
        "owenvoke/blade-fontawesome": "^2.4"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26",
        "callcocam/profile": "dev-main",
        "callcocam/tall-sluggable": "dev-main"
    },
    "autoload": {
        "files": [
            "helpers.php"
        ], 
        "psr-4": {
            "Callcocam\\Tenant\\": "src/",
            "Callcocam\\Tenant\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Callcocam\\Tenant\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Callcocam\\Tenant\\TenantServiceProvider"
            ],
            "aliases": {
                "Tenant": "Callcocam\\Tenant\\Facades\\Tenant"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}